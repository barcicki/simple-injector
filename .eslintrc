{
  "extends": ["airbnb-base"],
  "env": {
    "node": true,
    "jasmine": true
  },
  "rules": {
    "indent": ["error", 4],
    "comma-dangle": ["error", "never"],
    "no-param-reassign": ["error", { "props": false }],
    "no-restricted-syntax": ["error", "FunctionExpression", "WithStatement"]
  }
}